  "escalation_policy": {
    "to_human": [
      "Any matter scored 9-10 on litigation risk scale",
      "Tax filing submission (prepare only, human files)",
      "Medical license application submission",
      "Trade execution over $10K (require Telegram approval)",
      "Any output flagged as potential legal advice",
      "SEV-1 or SEV-2 security incidents",
      "Production deployment approval",
      "New client contract signing",
      "Agent deprecation or removal decisions"
    ],
    "to_premium_model": [
      "Client-facing deliverables (SOWs, reports)",
      "Contract review with red flags detected",
      "Complex multi-jurisdiction analysis",
      "Strategic financial modeling (M&A, fundraising)",
      "Security incident post-mortem analysis",
      "Product roadmap strategic decisions",
      "Competitive analysis with strategic recommendations"
    ]
